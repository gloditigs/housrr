                        <% rentalProperties.forEach(function(property) { %>
                            <article class="swiper-slide">
                                <div class="inner">
                                    <a href="/property/<%= property._id %>">

                                        <div class="media">
                                            <div class="inner-media">
                                                <img src="/uploads/<%= property.mainImage %>" alt="<%= property.title %>" width="200">
                                            </div>
                                        </div>
                                        

                                        <div class="text">
                                            <div class="room-price">
                                             
                                                <span class="current-price">Price: <span>POR</span></span>
                                            </div>

