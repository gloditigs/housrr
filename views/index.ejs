                                            <div class="room-details">
                                                <span><i class="icon-fi-maximize"></i><%= property.size %> ft<sup>2</sup></span>
                                                <span><i class="icon-fi-bed-6"></i><%= property.bedrooms %> Beds</span>
                                                <span><i class="icon-fi-bathtub-1"></i><%= property.bathrooms %> Bathrooms</span>
                                            </div>
                                        </div>
                                    </a>
                                </div>
                            </article>
                        <% }); %>

